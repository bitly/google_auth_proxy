# for heroku support (along with .godir)
web: google_auth_proxy --http-address=0.0.0.0:$PORT --redirect-url="$REDIRECT_URL" --client-id=$CLIENT_ID --client-secret="$CLIENT_SECRET" ${PASS_BASIC_AUTH:+--pass-basic-auth=$PASS_BASIC_AUTH} ${PASS_SECRET:+--pass-secret="$PASS_SECRET"} ${HTPASSWD_FILE:+--htpasswd-file="$HTPASSWD_FILE"} --cookie-secret="$COOKIE_SECRET" ${COOKIE_DOMAIN:+--cookie-domain=$COOKIE_DOMAIN} ${GOOGLE_APPS_DOMAIN:+--google-apps-domain=$GOOGLE_APPS_DOMAIN} ${AUTHENTICATED_EMAILS_FILE:+--authenticated-emails-file="$AUTHENTICATED_EMAILS_FILE"} ${REWRITE_HOST:+--rewrite-host=$REWRITE_HOST} $(for URL in $UPSTREAM; do printf " --upstream=%s" "$URL"; done)
